@model List<YUMMY.Net.Models.ContactInfo>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>İletişim</h2>

<table class="table table-bordered">
    <tr>
        <td>#</td>
        <td>Addres</td>
        <td>Mail</td>
        <td>Telefon Num.</td>
        <td>Harite Url</td>
        <td>Mesai Bilgileri</td>
        <td>Güncelle</td>
        <td>Sil</td>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.ContactInfoID</th>
            <th>@item.ADDRESS</th>
            <th>@item.Email</th>
            <th>@item.PhoneNumber</th>
            <th>@item.MapUrld</th>
            <th>@item.OpenHours</th>
            <td><a href="~/Contact/UpdateContact/@item.ContactInfoID"class="btn btn-success">Güncelle</a></td>
            <td><a href="~/Contact/DeleteContact/@item.ContactInfoID"class="btn btn-danger">Sil</a></td>

        </tr>

    }


</table>
<a href="#" class="btn btn-primary">İletişim EKLE</a>
