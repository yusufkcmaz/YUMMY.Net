@model  YUMMY.Net.Models.Feature


@{
    ViewBag.Title = "UpdateFeature";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Feature Güncelle</h2>

<br>
<form method="post">

    <label>Resim Url</label>
    @Html.TextBoxFor(x => x.ImageUrl, new { @class = "form-control" })
    <br />

    <label>Başlık</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />

    <label>Başlık 2 </label>
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
    <br />

    <label>Başlık 3 </label>
    @Html.TextBoxFor(x => x.VideoUrl, new { @class = "form-control" })
    <br />

    <button type="submit" class=" btn btn-primary">Kaydet</button>

</form>

